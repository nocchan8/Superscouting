          "type": "select",
          "required": false,
          "code": "passedHow",
          "formResetBehavior": "reset",
          "defaultValue": "blank",
          "choices": {
            "blank": "(blank)",
            "driving": "While driving",
            "stationary": "While stationary",
            "both": "Both",
            "none": "No Passing"
